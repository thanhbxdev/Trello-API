{
  "name": "trello-api",
  "version": "1.0.0",
  "description": "API for trello clone app",
  "main": "mongodb.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint src",
    "clean": "rimraf ./build && mkdirp ./build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/server.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thanhbxdev/Trello-API.git"
  },
  "keywords": [],
  "author": "thanhbxdev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thanhbxdev/Trello-API/issues"
  },
  "homepage": "https://github.com/thanhbxdev/Trello-API#readme",
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "eslint": "^7.32.0",
    "express": "^4.17.1",
    "mkdirp": "^1.0.4",
    "mongodb": "^4.1.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "nodemon": "^2.0.12"
  }
}
